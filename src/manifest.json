{
  "manifest_version": 3,
  "name": "InstaBookmark - 即刻书签",
  "description": "极简、方便，帮助你更快地找到并打开书签",
  "version": "0.1",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/bookmark-16.png",
    "48": "images/bookmark-48.png",
    "128": "images/bookmark-128.png"
  },
  "permissions": ["bookmarks", "favicon"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+1",
        "mac": "Alt+1"
      }
    }
  }
}
