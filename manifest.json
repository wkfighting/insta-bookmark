{
  "manifest_version": 3,
  "name": "My Bookmarks",
  "description": "My Bookmarks",
  "version": "0.1",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "permissions": ["bookmarks", "favicon"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Space",
        "mac": "Alt+Space"
      }
    }
  }
}
